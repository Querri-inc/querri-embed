# Server — used by createSessionHandler() in your API route
# Find your API key at https://app.querri.com/settings/api-keys
QUERRI_API_KEY=your-api-key-here
QUERRI_ORG_ID=your-org-id-here
# QUERRI_URL=https://app.querri.com  # Optional, defaults to https://app.querri.com

# Client — used by the <QuerriEmbed> component's serverUrl prop
# Next.js exposes NEXT_PUBLIC_* vars to the client bundle
NEXT_PUBLIC_QUERRI_URL=https://app.querri.com
